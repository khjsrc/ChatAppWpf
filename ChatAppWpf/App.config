<?xml version="1.0" encoding="utf-8" ?>
<!--You need to fill this file with ServerAddress and ServerPort to be able to connect to a server.-->
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
  <appSettings>
    <add key="ServerAddress" value="127.0.0.1"/>
    <add key="ServerPort" value="11001"/>
  </appSettings>
</configuration>